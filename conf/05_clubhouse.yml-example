name: Clubhouse Example
mode: clubhouse
clubhouse:
  api_key: YOUR_CLUBHOUSE_API_KEY
  project_id: YOUR_PROJECT_ID
  cache: true

# Configure your workflow here
# Right now Clubhouse support in Seshat only handles Created and Done
# WARNING: In the future the plan is to be able to assign several statuses to the same step
# WARNING: This is not yet working, so make sure that in the left and the right the names are the same
Workflow:
  Created: Created
  Done: Done